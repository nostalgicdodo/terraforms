
doctype html

html(
	lang = "en"
)
	head
		meta(
			charset="utf-8"
		)
		meta(
			name="viewport"
			content="width=device-width, initial-scale=1.0, user-scalable=no"
		)
		meta(
			http-equiv="X-UA-Compatible"
			content="IE=edge, chrome=1"
		)
		link(
			rel="stylesheet"
			href="base.css"
		)
		block head
			title untitled

	body
		block body
			script(
				type="text/javascript"
				src="base.js"
			)
